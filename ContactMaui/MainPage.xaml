<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             
             xmlns:models="clr-namespace:ClassLibrary.Shared.Models;assembly=ClassLibrary.Shared"
             xmlns:viewmodels="clr-namespace:ContactMaui.ViewModels"
             x:DataType="{x:Type viewmodels:MainViewModel}"
             
             x:Class="ContactMaui.MainPage">

    <ScrollView>
        <StackLayout>
            <Button Text="Add new contact" Command="{Binding NavigateToContactAdd}" />
        </StackLayout>
    </ScrollView>

</ContentPage>
